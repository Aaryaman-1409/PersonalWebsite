---
import BaseLayout from "../layouts/BaseLayout.astro";
---

<style>
  .pdf-embed {
    width: 65vw;
  }
  @media (max-width: 800px) {
    .pdf-embed {
      width: 90vw;
    }
  }
</style>

<BaseLayout title="Resume" sideBarActiveItemID="cv">
  <div class="pdf-embed">
    <object
      class="pdf"
      data="https://aaryaman-1409.github.io/CV.pdf"
      width="100%"
      height="800px"
    >
    </object>
  </div>
</BaseLayout>
